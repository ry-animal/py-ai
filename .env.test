# Test environment for enterprise database adapters
ENVIRONMENT=test
ANTHROPIC_API_KEY=test-key

# Use local/development databases for testing
VECTOR_DB_TYPE=chroma
MONGODB_URL=mongodb://localhost:27017/py_ai_test
POSTGRES_URL=postgresql://postgres:password@localhost:5432/py_ai_test
REDIS_URL=redis://localhost:6379/0

# Multi-agent features
ENABLE_SMART_ORCHESTRATOR=true
ENABLE_HYBRID_AGENT=true
ENABLE_PYDANTIC_AGENT=true
ENABLE_LANGGRAPH_AGENT=true

# Development settings
LOG_LEVEL=DEBUG
STREAMING_ENABLED=true